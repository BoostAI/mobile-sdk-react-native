{
  "name": "@boostai/mobile-sdk-react-native",
  "version": "0.9.0",
  "description": "React Native wrapper for Boost.ai SDK (Android and iOS)",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native.config.js",
    "boostai-react-native.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!android/.gradle",
    "!android/libs",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "android",
    "ios",
    "boost",
    "boost.ai",
    "chatbot",
    "conversational-ai",
    "chat",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BoostAI/mobile-sdk-react-native.git"
  },
  "author": "Boost.ai",
  "license": "GPL-3.0",
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.60.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/react": "^19.2.7",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.39.2",
    "react": "19.2.3",
    "react-native": "0.83.1",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^5.9.3"
  },
  "codegenConfig": {
    "name": "BoostAISpec",
    "type": "all",
    "jsSrcsDir": "src/specs",
    "android": {
      "javaPackageName": "com.boostai.reactnative"
    },
    "ios": {
      "moduleName": "BoostAI"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  }
}
